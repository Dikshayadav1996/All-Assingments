#!/bin/bash -x

read -p "enter a number between 1 and 5 inclusive > "character

if [ "$character" = "1" ];
then
   echo "you entered one."

elif [ "$character" = "2" ];
then
    echo "you entered two."

elif [ "$character" = "3" ];
then
   echo "you entered three."

elif [ "$character" = "4" ];
then
    echo "you entered four."

else
    echo "you did not enter a number between 1 and 5."
fi


